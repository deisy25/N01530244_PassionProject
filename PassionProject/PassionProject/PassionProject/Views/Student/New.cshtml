@model IEnumerable<PassionProject.Models.StudentDto>

@{
    ViewBag.Title = "Create";
}

<h2>Add Student</h2>
<form action="/Student/Create" method="POST">
    <div>
        <label>Name:</label>
        <input type="text" name="StudentName" placeholder="Nasya Pakasi" />
    </div>
    <div>
        <label>Date of Birth:</label>
        <input type="text" name="DateOfBirth" placeholder="03/25/1999" />
    </div>
    <div>
        <label>Address:</label>
        <input type="text" name="Address" placeholder="505-1600 Keele St" />
    </div>
    <div>
        <label>PostalCode:</label>
        <input type="text" name="PostalCode" placeholder="M6N 5J1 " />
    </div>
    <div>
        <input type="submit" value="Add Student" />
    </div>
</form>

